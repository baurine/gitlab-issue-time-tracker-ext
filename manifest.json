{
  "name": "Time Tracker for Gitlab Issue",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Help track gitlab issue time",
  "author": "baurine",
  "icons": {
    "128": "icons/icon_128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["time-tracker.js"]
      }
  ],
  "permissions": [
    "storage"
  ]
}
