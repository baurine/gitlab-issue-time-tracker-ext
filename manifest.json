{
  "name": "Time Tracker for Gitlab Issue",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Help track gitlab issue time",
  "author": "baurine",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["time-tracker.js"]
      }
  ],
  "permissions": [
    "storage"
  ]
}
