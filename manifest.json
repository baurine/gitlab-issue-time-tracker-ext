{
  "name": "Time Tracker for Gitlab Issue",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Track gitlab issue time",
  "author": "baurine",
  "options_page": "options.html",
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["time-tracker.js"]
      }
  ],
  "permissions": [
    "storage"
  ]
}
